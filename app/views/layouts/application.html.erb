<!DOCTYPE html>
<html>
<head>
  <title><%= @title || "Ticktee" %></title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body style="background: #F6F6F6 url(images/apple_style_base.png) repeat 0 0;">

<% flash.each do |key, value| %>
<div class='flash' id='<%= key %>'>
<%= value %>
</div>
<% end %>
<h1><%= link_to "Ticketee", root_path %></h1>

<%= yield %>

</body>
</html>
